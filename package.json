    "build": "vite build",
    "docs:dev": "vitepress dev docs/v1.x",
    "docs:build": "vitepress build docs/v1.x",
    "docs:serve": "vitepress serve docs/v1.x"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-vitest": "^0.3.8",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitepress": "^1.0.0-rc.24",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AseasRoa/paintor.git"
  }
