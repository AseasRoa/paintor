{
  "name": "paintor",
  "version": "1.0.0",
  "author": {
    "name": "AseasRoa"
  },
  "description": "JavaScript View library for the browser and for the server",
  "license": "MIT",
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./lib/index.js",
  "browser": "./dist/bundle.js",
  "exports": {
    "types": "./types/index.d.ts",
    "browser": "./dist/bundle.js",
    "import": "./lib/index.js",
    "default": "./lib/index.js"
  },
  "imports": {
    "#paintor": {
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config .eslintrc --ext .js,.ts ./",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "build:tsc": "tsc",
    "docs:dev": "vitepress dev docs/v1.x",
    "docs:build": "vitepress build docs/v1.x",
    "docs:preview": "vitepress preview docs/v1.x",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-vitest": "^0.3.18",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "jsdom": "^23.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitepress": "^1.0.0-rc.32",
    "vitest": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AseasRoa/paintor"
  },
  "homepage": "https://github.com/AseasRoa/paintor",
  "keywords": [
    "javascript",
    "vanilla",
    "hyperscript",
    "view library",
    "frontend",
    "backend"
  ]
}
