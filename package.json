{
  "name": "paintor",
  "version": "1.4.3",
  "author": {
    "name": "AseasRoa"
  },
  "description": "100% JavaScript view library for the browser and for the server",
  "license": "MIT",
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./lib/index.js",
  "browser": "./dist/bundle.js",
  "exports": {
    "types": "./types/index.d.ts",
    "browser": "./dist/bundle.js",
    "import": "./lib/index.js",
    "default": "./lib/index.js"
  },
  "imports": {
    "#paintor": {
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config eslint.config.js ./",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^25.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.2",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AseasRoa/paintor.git"
  },
  "homepage": "https://aseasroa.github.io/paintor",
  "keywords": [
    "frontend",
    "backend",
    "templates",
    "components",
    "ui",
    "view",
    "vanilla",
    "hyperscript",
    "reactive"
  ]
}
