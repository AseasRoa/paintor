{
  "name": "paintor",
  "version": "1.0.0",
  "author": {
    "name": "AseasRoa"
  },
  "description": "JavaScript View library for the browser and for the server",
  "homepage": "https://github.com/AseasRoa/paintor",
  "keywords": [
    "javascript",
    "vanilla",
    "hyperscript",
    "view library",
    "frontend",
    "backend"
  ],
  "license": "MIT",
  "type": "module",
  "types": "./src/types/global.d.ts",
  "main": "./dist/paintor.js",
  "module": "./dist/paintor.js",
  "exports": "./dist/paintor.js",
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config .eslintrc --ext .js,.ts ./src",
