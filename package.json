{
  "name": "paintor",
  "version": "1.3.8",
  "author": {
    "name": "AseasRoa"
  },
  "description": "100% JavaScript view library for the browser and for the server",
  "license": "MIT",
  "type": "module",
  "types": "./types/index.d.ts",
  "main": "./lib/index.js",
  "browser": "./dist/bundle.js",
  "exports": {
    "types": "./types/index.d.ts",
    "browser": "./dist/bundle.js",
    "import": "./lib/index.js",
    "default": "./lib/index.js"
  },
  "imports": {
    "#paintor": {
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config .eslintrc --ext .js,.ts ./",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jsdoc": "^48.2.9",
    "eslint-plugin-vitest": "^0.4.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "jsdom": "^24.1.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AseasRoa/paintor.git"
  },
  "homepage": "https://aseasroa.github.io/paintor",
  "keywords": [
    "frontend",
    "backend",
    "templates",
    "components",
    "ui",
    "view",
    "vanilla",
    "hyperscript",
    "reactive"
  ]
}
