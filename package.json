{
  "name": "paintor",
  "version": "1.0.0",
  "author": {
    "name": "AseasRoa"
  },
  "description": "JavaScript View library for the browser and for the server",
  "homepage": "https://github.com/AseasRoa/paintor",
  "keywords": [
    "javascript",
    "vanilla",
    "hyperscript",
    "view library",
    "frontend",
    "backend"
  ],
  "license": "MIT",
  "type": "module",
  "types": "./src/types/global.d.ts",
  "main": "./src/index.js",
  "browser": "./dist/bundle.js",
  "exports": {
    "types": "./src/types/global.d.ts",
    "browser": "./dist/bundle.js",
    "require": "./dist/bundle.cjs.js",
    "import": "./src/index.js",
    "default": "./src/index.js"
  },
  "imports": {
    "#paintor": {
      "default": "./src/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "lint": "tsc --noEmit && eslint --config .eslintrc --ext .js,.ts ./src",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "build:tsc": "tsc",
    "docs:dev": "vitepress dev docs/v1.x",
    "docs:build": "vitepress build docs/v1.x",
    "docs:preview": "vitepress preview docs/v1.x"
  },
  "devDependencies": {
    "@types/node": "^20.9.3",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.54.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-vitest": "^0.3.10",
    "eslint-plugin-vitest-globals": "^1.4.0",
    "jsdom": "^22.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vitepress": "^1.0.0-rc.29",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AseasRoa/paintor.git"
  }
}
